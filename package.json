{
  "name": "@tickh/pi",
  "type": "module",
  "version": "0.0.27",
  "private": false,
  "packageManager": "pnpm@7.1.2",
  "description": "",
  "author": "TickHeart",
  "license": "MIT",
  "homepage": "https://github.com/TickHeart/pi#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TickHeart/pi"
  },
  "bugs": {
    "url": "https://github.com/TickHeart/pi/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "pi": "./bin/pi.mjs",
    "pif": "./bin/pif.mjs",
    "add": "./bin/add.mjs",
    "addf": "./bin/addf.mjs",
    "addw": "./bin/addw.mjs",
    "init": "./bin/init.mjs",
    "pre": "./bin/pre.mjs",
    "pu": "./bin/pu.mjs",
    "pc": "./bin/pc.mjs",
    "pr": "./bin/pr.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "stub": "unbuild --stub",
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build",
    "release": "bumpp --commit --push --tag && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "fast-glob": "^3.2.11",
    "inquirer": "^9.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/inquirer": "^8.2.1",
    "@types/node": "^17.0.45",
    "bumpp": "^7.2.0",
    "eslint": "^8.19.0",
    "esno": "^0.16.3",
    "execa": "^6.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.5.0",
    "pnpm": "^7.5.0",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.4",
    "unplugin-auto-import": "^0.7.2",
    "vitest": "^0.12.10"
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
