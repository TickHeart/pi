{
  "name": "@tickh/pi",
  "type": "module",
  "version": "0.9.0",
  "private": false,
  "packageManager": "pnpm@7.5.2",
  "description": "",
  "author": "TickHeart",
  "license": "MIT",
  "homepage": "https://github.com/TickHeart/pi#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TickHeart/pi"
  },
  "bugs": {
    "url": "https://github.com/TickHeart/pi/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "pi": "./bin/pi.mjs",
    "pif": "./bin/pif.mjs",
    "ad": "./bin/ad.mjs",
    "adf": "./bin/adf.mjs",
    "pio": "./bin/pio.mjs",
    "pis": "./bin/pis.mjs",
    "pios": "./bin/pios.mjs",
    "adw": "./bin/adw.mjs",
    "pu": "./bin/pu.mjs",
    "init": "./bin/init.mjs",
    "pr": "./bin/pr.mjs",
    "pre": "./bin/pre.mjs",
    "pc": "./bin/pc.mjs",
    "dd": "./bin/dd.mjs",
    "pt": "./bin/pt.mjs",
    "ptu": "./bin/ptu.mjs",
    "ec": "./bin/ec.mjs",
    "poo": "./bin/poo.mjs",
    "pvite": "./bin/pvite.mjs",
    "pb": "./bin/pb.mjs",
    "px": "./bin/px.mjs"
  },
  "files": [
    "dist",
    "bin",
    "cache"
  ],
  "scripts": {
    "stub": "unbuild --stub",
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build",
    "release": "bumpp --commit --push --tag && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "auto": "esno scripts/build.ts",
    "autoBuild": "pnpm run auto && pnpm run lint:fix && pnpm run release"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "cli-table3": "^0.6.2",
    "dayjs": "^1.11.4",
    "find-up": "^6.3.0",
    "fs-extra": "^10.1.0",
    "ini": "^3.0.0",
    "inquirer": "^9.0.2",
    "minimist": "^1.2.6",
    "pacote": "^13.6.1",
    "yaml": "^2.1.1",
    "ztshared": "^0.0.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/app-root-path": "^1.2.4",
    "@types/columnify": "^1.5.1",
    "@types/fs-extra": "^9.0.13",
    "@types/ini": "^1.3.31",
    "@types/inquirer": "^8.2.1",
    "@types/minimist": "^1.2.2",
    "@types/node": "^17.0.45",
    "@types/pacote": "^11.1.5",
    "bumpp": "^7.2.0",
    "eslint": "^8.21.0",
    "esno": "^0.16.3",
    "execa": "^6.1.0",
    "fast-glob": "^3.2.11",
    "husky": "^8.0.1",
    "lint-staged": "^12.5.0",
    "pnpm": "^7.8.0",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.6",
    "unplugin-auto-import": "^0.7.2",
    "vitest": "0.18.1"
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
